#!/usr/bin/env bash

function run() {
  local addin_name=$1
  ! validate && echo "[ERROR] Usage: potato remove [addin_name]" && return
  local addin_path=$HOME/.potato/enabled/$addin_name
  [ ! -f "$addin_path" ] && echo "[INFO] Addin not enabled." && return
  rm $addin_path
}
function __autocomplete() {
  ls $HOME/.potato/enabled
}
function validate {
  [ ! -z "$addin_name" ]
}